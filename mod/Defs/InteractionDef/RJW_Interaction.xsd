										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="logRulesRecipient" minOccurs="1">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="rulesStrings" minOccurs="1">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="li" minOccurs="1" maxOccurs="unbounded" type="xs:string" />
												</xs:sequence>
											</xs:complexType>
										</xs:element>
