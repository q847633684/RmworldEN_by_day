				</xs:element>
			</xs:sequence>
		</xs:sequence>
		<xs:attribute name="Class" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="rjw.InteractionExtension" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>

	<xs:simpleType name="rjwSextype">
